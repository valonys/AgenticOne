# AgenticOne Frontend Environment Configuration
# Copy this file to .env.local and update with your actual values

# =============================================================================
# GOOGLE OAUTH CONFIGURATION
# =============================================================================

# Google OAuth Client ID (REQUIRED)
VITE_GOOGLE_CLIENT_ID=your-google-client-id-here

# Google OAuth Client Secret (REQUIRED)
VITE_GOOGLE_CLIENT_SECRET=your-google-client-secret-here

# =============================================================================
# API CONFIGURATION
# =============================================================================

# Backend API URL (REQUIRED)
VITE_API_URL=http://localhost:8000

# Frontend URL for OAuth redirects (REQUIRED)
VITE_FRONTEND_URL=http://localhost:3000

# =============================================================================
# GEMINI API CONFIGURATION
# =============================================================================

# Google Gemini API Key (REQUIRED)
VITE_GEMINI_API_KEY=your-gemini-api-key-here

# =============================================================================
# DEVELOPMENT CONFIGURATION
# =============================================================================

# Enable debug mode (default: false)
VITE_DEBUG=false

# Enable guest mode (default: true)
VITE_ENABLE_GUEST_MODE=true

# =============================================================================
# SECURITY CONFIGURATION
# =============================================================================

# JWT Secret for token validation (REQUIRED)
VITE_JWT_SECRET=your-jwt-secret-here

# Token expiration time in minutes (default: 30)
VITE_TOKEN_EXPIRE_MINUTES=30
